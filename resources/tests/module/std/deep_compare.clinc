
(defun deep_compare (A b)
    (if (l A)
        (if (l b)
            (assign-lambda inner_result (deep_compare (f A) (f b))
                (if inner_result
                    inner_result
                    (deep_compare (r A) (r b))
                )
            )
            1
        )
        (if (l b)
            -1
            (if (> A b)
                1
                (- 0 (> b A))
            )
        )
    )
)

(defun deep< (a b)
    (= (deep_compare a b) -1)
)

(defun deep> (a b)
    (= (deep_compare a b) 1)
)

(defun deep= (a b)
    (= (deep_compare a b) 0)
)

(defun deep<= (a b)
    (not (deep> a b))
)

(defun deep>= (a b)
    (not (deep< a b))
)

(defun deep!= (a b)
    (not (deep= a b))
)
